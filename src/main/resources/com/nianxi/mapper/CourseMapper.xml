<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nianxi.mapper.CourseMapper">
    <select id="queryCourse" resultType="com.nianxi.entity.Course" parameterType="string">
        select * from course
        where Cno = #{Cno}
    </select>
</mapper>